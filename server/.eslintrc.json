        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "semi": "off",
        "no-console": "off",
        "linebreak-style": 0,
        "func-names":"off"
    }
}
